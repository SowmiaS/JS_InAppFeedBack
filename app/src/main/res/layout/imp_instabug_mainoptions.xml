<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/txt_instabug_mainoptions_reportbug"
        android:layout_width="match_parent"
        android:layout_height="@dimen/imp_instabug_textheight"
        android:text="@string/imp_instabug_mainoptions_reportbug"
        android:background="@android:color/darker_gray"
        android:textColor="@android:color/white"
        android:layout_margin="@dimen/imp_instabug_margin"
        android:gravity="center"/>

    <TextView
        android:id="@+id/txt_instabug_mainoptions_feedback"
        android:layout_width="match_parent"
        android:layout_height="@dimen/imp_instabug_textheight"
        android:text="@string/imp_instabug_mainoptions_feedback"
        android:background="@android:color/darker_gray"
        android:textColor="@android:color/white"
        android:layout_margin="@dimen/imp_instabug_margin"
        android:gravity="center"/>


    <TextView
        android:id="@+id/txt_instabug_mainoptions_dismiss"
        android:layout_width="match_parent"
        android:layout_height="@dimen/imp_instabug_textheight"
        android:text="@string/imp_instabug_mainoptions_dismiss"
        android:background="@android:color/darker_gray"
        android:textColor="@android:color/white"
        android:layout_margin="@dimen/imp_instabug_margin"
        android:gravity="center"/>

</merge>